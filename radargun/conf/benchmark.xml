<bench-config>
   <master
         bindAddress="${127.0.0.1:master.address}"
         port="${21032:master.port}"/>
   <benchmark
         initSize="${10:Islaves}"
         maxSize="${10:slaves}"
         increment="1">
      <DestroyWrapper
            runOnAllSlaves="true"/>
      <StartCluster
            staggerSlaveStartup="true"
            delayAfterFirstSlaveStarts="5000"
            delayBetweenStartingSlaves="1000"/>
      <ClusterValidation
            passiveReplication="false"
            partialReplication="true"/>
      <ClearCluster />
      <WebSessionWarmup
            passiveReplication="false"
            numberOfKeys="10000"
            sizeOfValue="1000"
            numOfThreads="2"
            transactionSize="1"
            />
      <ResetStats />
      <WebSessionBenchmark
            perThreadSimulTime="1000"
            writeTxWorkload="10,10;10,10"
            readTxWorkload="20"
            noContention="false"
            writeTransactionPercentage="1"
            coordinatorParticipation="true"
            />
      <CacheSize
            statName="CACHE_SIZE_BEFORE_BENCH" />
      <CsvReportGeneration/>
   </benchmark>
   <products>
      <infinispan4>
         <config name="ispn.xml"/>
      </infinispan4>
   </products>
   <reports>
      <report name="Reports" />
   </reports>
</bench-config>
